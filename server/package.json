{
  "name": "map-hospitals",
  "version": "1.0.0",
  "description": "",
  "main": "app.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "run": "nodemon dist/src/app.js",
    "build:watch": "tsc --watch",
    "start": "cross-env NODE_ENV=default node app.js",
    "dev:server": "nodemon  src/app.ts",
    "client:install": "npm install --prefix client",
    "client:build": "npm run build --prefix client",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run server\" \"npm run client\"",
    "typeorm": "typeorm-ts-node-commonjs",
    "migrations:generate": "npm run typeorm migration:generate -- -d src/typeorm.config.ts src/migrations/auto",
    "migrations:run": "npm run typeorm migration:run -- -d src/typeorm.config.ts",
    "migrations:revert": "npm run typeorm migration:revert -- -d src/typeorm.config.ts",
    "migrations:create": "npm run typeorm migration:create ./src/migrations/userInit"
  },
